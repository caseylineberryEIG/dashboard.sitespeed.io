{
  "extends": "/config/secrets.json",
  "browsertime": {
    "connectivity": {
      "engine": "throttle",
      "profile": "cable"
    },
    "visualElements": true,
    "iterations": 3,
    "browser": "chrome",
    "video": false,
    "visualMetrics": true,
    "chrome": {
      "collectConsoleLog": true,
      "enableTraceScreenshots": false
    },
    "screenshot": false
  },
  "cpu": true,
  "gzipHAR": true,
  "html": {
    "fetchHARFiles": true,
    "showScript": false,
    "compareURL": "https://compare.sitespeed.io/"
  },
  "screenshot": {
    "type": "jpg"
  }
}
